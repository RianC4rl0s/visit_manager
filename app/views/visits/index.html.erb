<%= render partial: "shared/appBar" %>
<p style="color: green"><%= notice %></p>
<h1>Visitas</h1>
<div class="d-flex flex-row-reverse">
  <%= link_to "Nova Visita", new_visit_path, class:"btn btn-primary mx-2" %>
</div>
<hr/>
<div id="visits">
  <% @visits.each do |visit| %>
    <%= render visit %>
    <p>
      <%= link_to "Show this visit", visit %>
    </p>
  <% end %>
</div>
